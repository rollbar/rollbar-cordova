{
  "name": "rollbar-cordova",
  "version": "0.1.0",
  "description": "Rollbar offical supported plugin for Cordova. Provides full Rollbar functionality for Javascript, and native iOS and Android environments.",
  "repository": {
    "type": "git",
    "url": "http://github.com/rollbar/rollbar-cordova"
  },
  "cordova": {
    "id": "rollbar-cordova",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "keywords": [
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova",
    "rollbar",
    "plugin"
  ],
  "engines": [
    {
      "name": "cordova",
      "version": ">=3.6.0"
    }
  ],
  "scripts": {
    "prepare": "cp node_modules/rollbar/dist/rollbar.min.js lib/rollbar.js",
    "test": "echo 'Error: no test specified'"
  },
  "dependencies": {
    "rollbar": "^2.6.1"
  },
  "license": "MIT"
}
